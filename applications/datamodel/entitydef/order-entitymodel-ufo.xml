<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
  <!-- ========================================================= -->
  <!-- ======================== Defaults ======================= -->
  <!-- ========================================================= -->
    <title>Entity of an Open For Business Project Component</title>
    <description>None</description>
    <copyright>Copyright 2001-2020 The Apache Software Foundation</copyright>
    <author>None</author>
    <version>1.0</version>

    <view-entity entity-name="CustRqStatus" dependent-on="StatusItem"
            package-name="org.apache.ofbiz.order.request">
        <member-entity entity-alias="PE" entity-name="StatusItem"/>
        <alias-all entity-alias="PE"/>
        <entity-condition>
            <condition-expr entity-alias="PE" field-name="statusTypeId" operator="equals" value="CUSTREQ_STTS"/>
            <order-by field-name="sequenceId"/>
        </entity-condition>
    </view-entity>
    <extend-entity entity-name="CustRequest">
        <relation rel-entity-name="CustRqStatus" type="one-nofk" >
            <key-map field-name="statusId" />
        </relation>
    </extend-entity>
    <extend-entity entity-name="CustRequestItem">
        <relation rel-entity-name="CustRqStatus" type="one-nofk" >
            <key-map field-name="statusId" />
        </relation>
    </extend-entity>

</entitymodel>

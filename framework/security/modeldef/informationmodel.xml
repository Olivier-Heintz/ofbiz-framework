    <stereotype name="java-object"/>
    <association-class participant2="UserLogin" participant1="SecurityGroup"/>
  </entity>
  <entity entity-name="ProtectedView" package-name="org.apache.ofbiz.security.securitygroup">
    <stereotype name="java-object"/>
    <relation with="SecurityGroup" type="composite"/>
  </entity>
  <entity entity-name="TarpittedLoginView" package-name="org.apache.ofbiz.security.securitygroup">
    <stereotype name="java-object"/>
  </entity>

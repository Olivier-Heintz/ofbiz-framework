    <relation with="UserLogin" type="composite"/>
  </entity>
  <entity entity-name="UserLoginHistory" package-name="org.apache.ofbiz.security.login">
    <stereotype name="java-object"/>
    <relation with="UserLogin" type="composite"/>
  </entity>
  <entity entity-name="UserLoginSession" package-name="org.apache.ofbiz.security.login">
    <stereotype name="java-object"/>
    <relation with="UserLogin" type="composite"/>
  </entity>
  <entity entity-name="SecurityGroup" package-name="org.apache.ofbiz.security.securitygroup">
    <stereotype name="java-object"/>
  </entity>
  <entity entity-name="SecurityGroupPermission" package-name="org.apache.ofbiz.security.securitygroup">
    <stereotype name="java-object"/>
    <relation with="SecurityGroup" type="composite"/>
  </entity>
  <entity entity-name="SecurityPermission" package-name="org.apache.ofbiz.security.securitygroup">
    <stereotype name="java-object"/>
  </entity>

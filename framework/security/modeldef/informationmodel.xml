<?xml version="1.0"?>
<informationmodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2000/10/XMLSchema" xsi:noNamespaceSchemaLocation="http://repository.ofbizextra.org/dtd/Informationmodel.xsd">

  <entity entity-name="X509IssuerProvision" package-name="org.apache.ofbiz.security.cert">
